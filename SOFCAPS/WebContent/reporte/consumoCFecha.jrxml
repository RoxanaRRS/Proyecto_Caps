		<queryString language="SQL">
			<![CDATA[select o.fecha_fin, o.consumoTotal, o.lectura_Actual, o.Consumo_ID, 
e.Cliente_ID, concat(e.nombre1,' ', e.nombre2,' ', e.apellido1, ' ',e.apellido2) as nombreCompleto, c.Contrato_ID, c. numContrato, c.numMedidor
,o.eliminado,e.estado,c.estado
from consumo o 
inner join cliente e on o.Cliente_ID=e.Cliente_ID
inner join contrato c on c.Contrato_ID=o.Contrato_ID where o.eliminado=0 and e.estado=0 and c.estado=0]]>
		</queryString>
		<field name="fecha_fin" class="java.sql.Date"/>
		<field name="consumoTotal" class="java.lang.Float"/>
		<field name="lectura_Actual" class="java.lang.Float"/>
		<field name="Consumo_ID" class="java.lang.Integer"/>
		<field name="Cliente_ID" class="java.lang.Integer"/>
		<field name="nombreCompleto" class="java.lang.String"/>
		<field name="Contrato_ID" class="java.lang.Integer"/>
		<field name="numContrato" class="java.lang.Integer"/>
		<field name="numMedidor" class="java.lang.String"/>
		<field name="eliminado" class="java.lang.Boolean"/>
		<field name="estado" class="java.lang.Boolean"/>
		<field name="COLUMN_12" class="java.lang.Boolean"/>
	</subDataset>
	<parameter name="Parameter1" class="java.lang.String"/>
	<parameter name="Parameter2" class="java.lang.String"/>
	<parameter name="userC" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[
SELECT 
        CONCAT(`cliente`.`nombre1`,
                ' ',
                `cliente`.`nombre2`,
                ' ',
                `cliente`.`apellido1`,
                ' ',
                `cliente`.`apellido2`) AS `nombreCompleto`,
        `contrato`.`numMedidor` AS `numMedidor`,
        `consumo`.`lectura_Actual` ,
        `consumo`.`consumoTotal`,
          SUM(round(`factura_detalle`.`consumo` * `factura_detalle`.`monto`,2)) AS `totalPago`,
         `sector`.`nombreSector` AS `sector`,
         `consumo`.`fecha_fin` 
         
FROM
		(`consumo`
        Left JOIN `contrato` ON (`consumo`.`Contrato_ID` = `contrato`.`Contrato_ID`)
        Left JOIN `cliente` ON (`contrato`.`Cliente_ID` = `cliente`.`Cliente_ID`)
        Left JOIN `sector` ON (`contrato`.`Sector_ID` = `sector`.`Sector_ID`)
        Left JOIN `factura_maestra` ON (`consumo`.`Consumo_ID` = `factura_maestra`.`Consumo_ID`)
        Left JOIN `factura_detalle` ON (`factura_maestra`.`Factura_Maestra_ID` = `factura_detalle`.`Factura_Maestra_ID`))
        
where consumo.eliminado=0 and cliente.estado=0 and contrato.estado=0 and consumo.fecha_fin  between  $P{Parameter1}   and  $P{Parameter2}
GROUP BY ((((((`cliente`.`nombre1` + ' ') + `cliente`.`nombre2`) + ' ') + `cliente`.`apellido1`) + ' ') + `cliente`.`apellido2`) , `contrato`.`numMedidor`, `consumo`.`lectura_Actual`,  `consumo`.`consumoTotal`, `sector`.`nombreSector` , `consumo`.`fecha_fin`;]]>
	</queryString>
