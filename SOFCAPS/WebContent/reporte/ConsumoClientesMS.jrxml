	<parameter name="mini" class="java.lang.String"/>
	<parameter name="aini" class="java.lang.String"/>
	<parameter name="nomini" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
        CONCAT(`cliente`.`nombre1`,
                ' ',
                `cliente`.`nombre2`,
                ' ',
                `cliente`.`apellido1`,
                ' ',
                `cliente`.`apellido2`) AS `nombre_completo`,
        `contrato`.`numMedidor` AS `numMedidor`,
        `consumo`.`lectura_Actual` AS `lectura_mes`,
        `consumo`.`consumoTotal` AS `consumo_mes`,
          SUM(`factura_detalle`.`consumo` * `factura_detalle`.`monto`) AS `totalPago`,
         `sector`.`nombreSector` AS `sector`,
         `consumo`.`fecha_fin` AS `fecha_mes`
         
FROM
		(`consumo`
        Left JOIN `contrato` ON (`consumo`.`Contrato_ID` = `contrato`.`Contrato_ID`)
        Left JOIN `cliente` ON (`contrato`.`Cliente_ID` = `cliente`.`Cliente_ID`)
        Left JOIN `sector` ON (`contrato`.`Sector_ID` = `sector`.`Sector_ID`)
        Left JOIN `factura_maestra` ON (`consumo`.`Consumo_ID` = `factura_maestra`.`Consumo_ID`)
        Left JOIN `factura_detalle` ON (`factura_maestra`.`Factura_Maestra_ID` = `factura_detalle`.`Factura_Maestra_ID`))
        
Where
	consumo.fecha_fin between  $P{mini} and  $P{aini} 
	and sector.nombreSector= $P{nomini}]]>
	</queryString>
