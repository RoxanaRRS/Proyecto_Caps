	<parameter name="mesIniciojasper" class="java.lang.String"/>
	<parameter name="mesFinjasper" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
        CONCAT(`cliente`.`nombre1`,
                ' ',
                `cliente`.`nombre2`,
                ' ',
                `cliente`.`apellido1`,
                ' ',
                `cliente`.`apellido2`) AS `nombre_completo`,
        `contrato`.`numMedidor` AS `numMedidor`,
        `consumo`.`lectura_Actual` AS `lectura_mes`,
        `consumo`.`consumoTotal` AS `consumo_mes`,
         
         `sector`.`nombreSector` AS `sector`,
         `consumo`.`fecha_fin` AS `fecha_mes`
         
FROM
		`consumo`
        JOIN `contrato` ON (`consumo`.`Contrato_ID` = `contrato`.`Contrato_ID`)
        JOIN `cliente` ON (`contrato`.`Cliente_ID` = `cliente`.`Cliente_ID`)
        JOIN `sector` ON (`contrato`.`Sector_ID` = `sector`.`Sector_ID`)
        
Where
	consumo.fecha_fin between   $P{mesIniciojasper} and  $P{mesFinjasper} 
	and consumo.eliminado=0]]>
	</queryString>
